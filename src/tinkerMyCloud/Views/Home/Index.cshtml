@{
    ViewData["Title"] = "Home Page";
}

<div>
    <h1>Tinker upload file</h1>
    <p>api uri get all (json): http://localhost:0000/api/Rest </p>
</div>

<div>
    @using (Html.BeginForm("FileUpload", "Home", FormMethod.Post, new { enctype = "multipart/form-data", id = "SubmitTarget" }))
    {
        @Html.ValidationSummary();

        <p> Välj en fil att ladda upp</p>
        <div>
            <input type="file" id="fileToUpload" name="file" />
        </div>

        <div>
            <button type="submit" class="btn btn-default" value="Upload">Lägg till fil</button>
        </div>
    }
</div>